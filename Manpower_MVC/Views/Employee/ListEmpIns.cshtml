@model IEnumerable<Manpower_MVC.ViewModels.ViewEmpIns>

@{
    ViewBag.Title = "勞健加減項";
}

<h2>ListEmpIns</h2>

<p>
    @Html.ActionLink("增新", "CreateEmpIns", new { id = ViewBag.empId })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.InsID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InsName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PosOrNeg)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Remark)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.InsID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InsName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PosOrNeg)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Remark)
        </td>
        <td>
            @Html.ActionLink("更新", "EditEmpIns", new { id=item.ID }) |
            @Html.ActionLink("刪除", "DeleteEmpIns", new { id=item.ID })
        </td>
    </tr>
}

</table>
